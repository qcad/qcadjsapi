    <function name="computePropertyValue" static="true">
      <variant return-type="void" static="true" access="protected">
        <parameters>
          <parameter const="false" type="RProperty" modifier="&amp;" name="property" />
        </parameters>
      </variant>
    </function>
    -->
  </class>
